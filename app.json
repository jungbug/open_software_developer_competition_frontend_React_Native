{
  "expo": {
    "name": "healthwatch_oss",
    "slug": "healthwatch_oss",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.act.healthwatch",
      "config": {
        "usesCleartextTraffic": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "cleartext": {
          "include": "http://openapi.foodsafetykorea.go.kr"
        }
      },
      "package": "com.act.healthwatch"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1dae61bc-545f-4d3d-a950-48f9118a2433"
      }
    }
  }
}
